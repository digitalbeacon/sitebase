<default property="null" type="string" />
<default headerClass="null" type="string" />
<default format="null" type="string" />

<%
grid.Columns(columns =>
{
	var c = columns.Bound(property);
	if (headerClass.HasText()) {
		c.HeaderHtmlAttributes(new { @class = headerClass });
	}
	if (format.HasText()) {
		c.Format(format);
	}
});
%>