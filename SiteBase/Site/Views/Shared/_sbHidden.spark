<default clientFormPanel="false" />
<default clientFormData="'formData'" />

<if condition="clientFormPanel">
	<default clientPropertyName="'{0}.{1}'.FormatWith(clientFormData, propertyName)" />
	!{Html.Hidden(propertyName, null, new { ng_model = clientPropertyName, ng_init = "{0}='{1}'".FormatWith(clientPropertyName, ModelMetadata.FromStringExpression(propertyName, ViewData).Model) })}
</if>
<else>
	${Html.Hidden(propertyName)}
</else>
